{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
    }
  },
  "installCommand": "npm install --legacy-peer-deps --force",
  "buildCommand": "npm run build"
}